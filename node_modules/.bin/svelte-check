#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/johannesameebjorkdahl/Documents/repos/johannesamee-web/node_modules/.pnpm/svelte-check@3.6.4_postcss-load-config@4.0.2_postcss@8.4.38__postcss@8.4.38_svelte@4.2.10/node_modules/svelte-check/bin/node_modules:/Users/johannesameebjorkdahl/Documents/repos/johannesamee-web/node_modules/.pnpm/svelte-check@3.6.4_postcss-load-config@4.0.2_postcss@8.4.38__postcss@8.4.38_svelte@4.2.10/node_modules/svelte-check/node_modules:/Users/johannesameebjorkdahl/Documents/repos/johannesamee-web/node_modules/.pnpm/svelte-check@3.6.4_postcss-load-config@4.0.2_postcss@8.4.38__postcss@8.4.38_svelte@4.2.10/node_modules:/Users/johannesameebjorkdahl/Documents/repos/johannesamee-web/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/johannesameebjorkdahl/Documents/repos/johannesamee-web/node_modules/.pnpm/svelte-check@3.6.4_postcss-load-config@4.0.2_postcss@8.4.38__postcss@8.4.38_svelte@4.2.10/node_modules/svelte-check/bin/node_modules:/Users/johannesameebjorkdahl/Documents/repos/johannesamee-web/node_modules/.pnpm/svelte-check@3.6.4_postcss-load-config@4.0.2_postcss@8.4.38__postcss@8.4.38_svelte@4.2.10/node_modules/svelte-check/node_modules:/Users/johannesameebjorkdahl/Documents/repos/johannesamee-web/node_modules/.pnpm/svelte-check@3.6.4_postcss-load-config@4.0.2_postcss@8.4.38__postcss@8.4.38_svelte@4.2.10/node_modules:/Users/johannesameebjorkdahl/Documents/repos/johannesamee-web/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../svelte-check/bin/svelte-check" "$@"
else
  exec node  "$basedir/../svelte-check/bin/svelte-check" "$@"
fi
